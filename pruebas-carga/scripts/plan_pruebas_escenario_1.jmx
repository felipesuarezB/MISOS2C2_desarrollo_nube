            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Generar un username único para cada ejecución
def timestamp = System.currentTimeMillis()
def threadNum = ctx.getThreadNum()
def random = new Random().nextInt(100000)
def uniqueUsername = "test_" + timestamp + "_" + threadNum + "_" + random

// Guardar en una variable para usar en la solicitud
vars.put("dynamic_username", uniqueUsername)
</stringProp>
